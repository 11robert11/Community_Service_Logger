{"changed":true,"filter":false,"title":"new.html.erb","tooltip":"/app/views/users/new.html.erb","value":"<% provide(:title, 'Form A') %>\n\n    \n<html lang=\"en\">\n\n  <head>\n\n  <meta charset=\"utf-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, shrink-to-fit=no\">\n  <meta name=\"description\" content=\"\">\n  <meta name=\"author\" content=\"\">\n\n  <!-- Font Awesome Icons -->\n  <link href=\"/assets/startbootstrap-creative/vendor/fontawesome-free/css/all.min.css\" rel=\"stylesheet\" type=\"text/css\">\n\n  <!-- Google Fonts -->\n  <link href=\"https://fonts.googleapis.com/css?family=Merriweather+Sans:400,700\" rel=\"stylesheet\">\n  <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic' rel='stylesheet' type='text/css'>\n\n  <!-- Plugin CSS -->\n  <link href=\"/assets/startbootstrap-creative/vendor/magnific-popup/magnific-popup.css\" rel=\"stylesheet\">\n\n  <!-- Theme CSS - Includes Bootstrap -->\n  <link href=\"/assets/startbootstrap-creative/css/creative.min.css\" rel=\"stylesheet\">\n  \n\n  </head>\n\n  <body id=\"page-top\">\n\n    <!-- Navigation -->\n\n\n    <header class=\"masthead text-center text-white d-flex\">\n      <div class=\"container my-auto\">\n        <div class=\"row\">\n          <div class=\"col-lg-10 mx-auto\">\n            <h1 class=\"text-uppercase\">\n              <strong>CVHS Community Service Logger</strong>\n            </h1>\n            <hr>\n            <h2 class=\"text-uppercase\">\n              <strong>ALL STUDENTS MUST COMPLETE AND SUBMIT THIS FORM BEFORE BEGINNING SERVICE.</strong>\n            </h2>\n          </div>\n          <div class=\"col-lg-8 mx-auto\">\n            <p class=\"text-faded mb-5\">The CVHS Community Service Learning Project encourages students to volunteer their time to help others by performing community service. We believe this program enhances skills, builds character and fosters personal growth. Participation is strongly encouraged but is not required. </p>\n            <p class=\"text-faded mb-5\">As a participant in the CVHS Community Service Learning Project, I realize that my behavior in the community is a reflection of my school. I promise to observe CVHS rules of conduct and to behave in a manner that will make my school proud to send me into the community as a representative of CVHS. </p>\n            <a class=\"btn btn-primary btn-xl js-scroll-trigger\" href=\"#signup\">Sign up now</a>\n          </div>\n        </div>\n      </div>\n    </header>\n    \n    <section id=\"signup\">\n      <div class=\"container text-center\">\n        <h2 class=\"mb-4\">Sign up</h2>\n        <hr>\n        <div class=\"row\">\n          <div class=\"container text-left\">\n            <%= simple_form_for @user do |f| %>\n              <%= render 'shared/error_messages', object: f.object %>\n                <%= f.label :first_name, \"First Name\" %>\n                <%= f.text_field :first_name, class: 'form-control'  %>\n          \n                <%= f.label :last_name, \"Last Name\" %>\n                <%= f.text_field :last_name, class: 'form-control'  %>\n          \n                <%= f.label :email, \"Student Email\" %>\n                <%= f.email_field :email, class: 'form-control'  %>\n            \n                <%= f.label :student_id, \"Student ID (6-digit)\" %> \n                <%= f.text_field :student_id, class: 'form-control'  %>\n          \n                <%= f.label :grade, \"Class of: (Graduating Year)\" %>\n                <%= f.text_field :grade, class: 'form-control'  %>\n          \n                <%= f.label :password, \"Password (At least 6 characters)\"%>\n                <%= f.password_field :password, class: 'form-control' %>\n          \n                <%= f.label :password_confirmation, \"Password Confirmation\" %>\n                <%= f.password_field :password_confirmation, class: 'form-control' %>\n          </div>\n        </div>\n        <%= f.button :submit, 'Create Account', class: 'btn btn-success'  %>\n                \n        <% end %>\n        <br>\n                            \n          <p>Already have an account? <%= link_to \"Log in!\", user_login_path %></p>\n          <p>Have any questions? Contact cvcommunityservice@gmail.coms</p>\n          <br>\n          <p>Supervisor? click <%= link_to \"here\", formB_path %> to create an account</p>\n        </div>\n        </div>\n        </div>\n        </div>\n    </section>\n    \n    \n\n  <!-- Bootstrap core JavaScript -->\n  <script src=\"/assets/startbootstrap-creative/vendor/jquery/jquery.min.js\"></script>\n  <script src=\"/assets/startbootstrap-creative/vendor/bootstrap/js/bootstrap.bundle.min.js\"></script>\n\n  <!-- Plugin JavaScript -->\n  <script src=\"/assets/startbootstrap-creative/vendor/jquery-easing/jquery.easing.min.js\"></script>\n  <script src=\"/assets/startbootstrap-creative/vendor/magnific-popup/jquery.magnific-popup.min.js\"></script>\n\n  <!-- Custom scripts for this template -->\n  <script src=\"/assets/startbootstrap-creative/js/creative.min.js\"></script>\n  \n  </body>\n\n</html>\n","undoManager":{"mark":21,"position":22,"stack":[[{"start":{"row":89,"column":83},"end":{"row":90,"column":0},"action":"insert","lines":["",""],"id":2},{"start":{"row":90,"column":0},"end":{"row":90,"column":10},"action":"insert","lines":["          "]},{"start":{"row":90,"column":10},"end":{"row":90,"column":11},"action":"insert","lines":["<"]},{"start":{"row":90,"column":11},"end":{"row":90,"column":12},"action":"insert","lines":["p"]}],[{"start":{"row":90,"column":12},"end":{"row":90,"column":13},"action":"insert","lines":[" "],"id":3}],[{"start":{"row":90,"column":12},"end":{"row":90,"column":13},"action":"remove","lines":[" "],"id":4}],[{"start":{"row":90,"column":12},"end":{"row":90,"column":17},"action":"insert","lines":["></p>"],"id":5}],[{"start":{"row":90,"column":13},"end":{"row":90,"column":14},"action":"insert","lines":[" "],"